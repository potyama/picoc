#Picoc09 2011-12-13
__start__:
	call	main
	halt
#組込み関数 read()
#ここに read() の機械語を書く
read:
	enter
	rd
	storel	2
	leave
	ret
#組込み関数 write()
#ここに write() の機械語を書く
write:
	enter
	pushl	3
	wr
	leave
	ret
#組込み関数 writeln()
writeln:
	enter
	wrln
	leave
	ret
main:
	enter
	mvsp	-2
	pushi	0
	storel	-1
	mvsp	1
	pushl	-1
	pushi	100
	eq
	jf	.L0
	pushi	100
	storel	-2
	mvsp	1
	jp	.L1
.L0:
	pushi	0
	storel	-2
	mvsp	1
.L1:
	pushl	-2
	mvsp	-1
	call	write
	storet	1
	mvsp	1
	mvsp	1
	pushl	-2
	storel	2
	mvsp	2
	leave
	ret
